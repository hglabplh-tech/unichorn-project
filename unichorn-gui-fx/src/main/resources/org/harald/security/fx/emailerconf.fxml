            <TextField fx:id="name" GridPane.columnIndex="1" />
            <TextField fx:id="alias" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="storeLoc" GridPane.columnIndex="1" GridPane.rowIndex="1">
