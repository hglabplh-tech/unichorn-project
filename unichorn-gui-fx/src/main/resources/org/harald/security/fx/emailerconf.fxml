                  <ContextMenu>
                    <items>
                      <MenuItem mnemonicParsing="false" onAction="#selectStore" text="Select Store" />
                    </items>
                  </ContextMenu>
               </contextMenu>
            </TextField>
            <Button fx:id="back" mnemonicParsing="false" onAction="#back" text="Go Back" GridPane.columnIndex="1" GridPane.rowIndex="7" />
            <Button fx:id="save" mnemonicParsing="false" onAction="#save" text="Sava" GridPane.rowIndex="7" />
