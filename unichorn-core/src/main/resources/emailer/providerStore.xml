<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<mailboxes  xmlns="http://org.harry.security/emailer/1.0">
    <mailbox configName="t-online">
        <imapHost>secureimap.t-online.de</imapHost>
        <imapPort>993</imapPort>
        <smtpHost>securesmtp.t-online.de</smtpHost>
        <smtpPort>465</smtpPort>
    </mailbox>
    <mailbox configName="gmx">
        <imapHost>imap.gmx.net</imapHost>
        <imapPort>993</imapPort>
        <smtpHost>mail.gmx.net </smtpHost>
